	#include<stdio.h>
  main()
  {
  printf("pick an item:\n1. chips\n2.ice creamn\n3. puff\n4. pastry");
	int choice=0;
	scanf("%d",&choice);
	
	switch(choice)
	{
		case 1:
		   printf("you picked chips.");
		   break;
	    case 2:
		   printf("you picked ice cream.");
		   break;
	    case 3:
		   printf("you picked puff.");
		   break;
	    case 4:
		   printf("you picked pastry.");
		   break;
		default: printf("invalid choice");	
	}
}
